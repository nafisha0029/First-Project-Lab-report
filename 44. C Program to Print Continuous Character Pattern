#include <stdio.h>
 int main() {
    int n, i, j;
    char c = 'A';
    printf("Enter number of rows: ");
    if (scanf("%d", &n) != 1) return 0;
    for (i = 0; i < n; i++) {
        for (j = 0; j <= i; j++) {
            if (c > 'Z') c = 'A';
            printf("%c ", c++);
        }
        putchar('\n');
    }
    return 0;
 }
