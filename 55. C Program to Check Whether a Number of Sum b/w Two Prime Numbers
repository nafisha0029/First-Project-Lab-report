#include <stdio.h>
 #include <math.h>
 int isprime(int n) {
    if (n < 2) return 0;
    for (int i = 2; i <= (int)sqrt(n); i++)
        if (n % i == 0) return 0;
    return 1;
 }
 int main() {
    int n;
    printf("Enter a number: ");
    if (scanf("%d", &n) != 1) return 0;
    int ok = 0;
    for (int i = 2; i <= n / 2; i++) {
        if (isprime(i) && isprime(n - i)) { ok = 1; break; }
    }
    if (ok) printf("%d can be expressed as sum of two primes\n", n);
    else printf("%d cannot be expressed as sum of two primes\n", n);
    return 0;
 }
