 #include <stdio.h>
 #include <math.h>
 int isprime(int n) {
    if (n < 2) return 0;
    for (int i = 2; i <= (int)sqrt(n); i++)
        if (n % i == 0) return 0;
    return 1;
 }
 int main() {
    int n;
    printf("Enter a number: ");
    if (scanf("%d", &n) != 1) return 0;
    if (isprime(n)) printf("%d is prime\n", n);
    else printf("%d is not prime\n", n);
    return 0;
 }
