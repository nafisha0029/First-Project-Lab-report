#include <stdio.h>
#include <string.h>

int main() {
    FILE *f = fopen("text.txt", "r");
    char line[200], pattern[50];

    printf("Enter pattern: ");
    scanf("%s", pattern);

    while(fgets(line, sizeof(line), f))
        if(strstr(line, pattern))
            printf("%s", line);

    fclose(f);
    return 0;
}
