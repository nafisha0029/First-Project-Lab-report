#include <stdio.h>
 #include <math.h>
 int isprime(int n) {
    if (n < 2) return 0;
    for (int i = 2; i <= (int)sqrt(n); i++)
        if (n % i == 0) return 0;
    return 1;
 }
 int main() {
    int low, high;
    printf("Enter two numbers (low high): ");
    if (scanf("%d %d", &low, &high) != 2) return 0;
    if (low > high) { int t = low; low = high; high = t; }
    printf("Primes between %d and %d:\n", low, high);
    for (int i = low; i <= high; i++) if (isprime(i)) printf("%d ", i);
    putchar('\n');
    return 0;

 }
