 #include <stdio.h>
 #include <stdlib.h>
 typedef struct {
    int *a;
    int top;
    int size;
 } Stack;
 void push(Stack *s, int v) {
    if (s->top < s->size - 1) s->a[++s->top] = v;
 }
 int pop(Stack *s) {
    if (s->top >= 0) return s->a[s->top--];
    return -1;
 }
 void insertAtBottom(Stack *s, int x) {
    if (s->top == -1) {
        push(s, x);
    } else {
        int y = pop(s);
        insertAtBottom(s, x);
        push(s, y);
    }
 }
 void reverseStack(Stack *s) {
    if (s->top != -1) {
        int x = pop(s);
        reverseStack(s);
        insertAtBottom(s, x);
    }
 }
 int main() {
    Stack s;
    s.size = 100;
    s.a = malloc(s.size * sizeof(int));
    s.top = -1;
    // Example pushes
    push(&s, 1);
    push(&s, 2);
    push(&s, 3);
    reverseStack(&s);
    while (s.top != -1) printf("%d ", pop(&s));
    printf("\n");
    free(s.a);
    return 0;
 }
