#include <stdio.h>
#include <math.h>
int main(){
    int a[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    int trace=0, sumsq=0;
    for(int i=0;i<3;i++){
        trace += a[i][i];
        for(int j=0;j<3;j++)
            sumsq += a[i][j]*a[i][j];
    }
    printf("Trace = %d\n", trace);
    printf("Normal = %.2f", sqrt(sumsq));
}
