#include <stdio.h>
#include <string.h>
int main(){
    char a[]="1011", b[]="1101";
    int i=strlen(a)-1,j=strlen(b)-1,carry=0,k=0;
    char res[100]; res[99]=0;
    k=98;
    while(i>=0||j>=0||carry){
        int sum = carry + (i>=0? a[i--]-'0':0) + (j>=0? b[j--]-'0':0);
        res[k--] = (sum%2)+'0'; carry=sum/2;
    }
    printf("%s\n",&res[k+1]);
    return 0;
}
