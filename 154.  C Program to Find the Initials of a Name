#include <stdio.h>

int main() {
    char name[100];

    printf("Enter full name: ");
    fgets(name, sizeof(name), stdin);

    printf("Initials: ");

    for(int i = 0; name[i] != '\0'; i++) {
        if(i == 0 || name[i - 1] == ' ')
            if(name[i] != ' ')
                printf("%c", name[i]);
    }

    return 0;
}
