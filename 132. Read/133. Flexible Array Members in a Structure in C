#include <stdio.h>
#include <stdlib.h>

struct Data {
    int size;
    int arr[];  
};

int main() {
    struct Data *d = malloc(sizeof(*d) + 5 * sizeof(int));
    d->size = 5;

    for(int i = 0; i < d->size; i++)
        d->arr[i] = i + 1;

    for(int i = 0; i < d->size; i++)
        printf("%d ", d->arr[i]);

    free(d);
    return 0;
}
