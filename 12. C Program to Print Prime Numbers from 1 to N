#include <stdio.h>
#include <math.h>
#include <stdbool.h>

bool isPrime(int n) {
    if (n <= 1) return false;
    if (n == 2) return true;
    if (n % 2 == 0) return false;
    int lim = (int)sqrt(n);
    for (int i = 3; i <= lim; i += 2)
        if (n % i == 0) return false;
    return true;
}

int main() {
    int N;
    printf("Enter N: ");
    if (scanf("%d", &N) != 1) return 0;
    printf("Prime numbers from 1 to %d:\n", N);
    for (int i = 2; i <= N; ++i)
        if (isPrime(i)) printf("%d ", i);
    printf("\n");
    return 0;
}
