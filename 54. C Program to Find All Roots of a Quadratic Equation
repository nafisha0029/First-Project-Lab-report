 #include <stdio.h>
 #include <math.h>
 int main() {
    double a, b, c;
    printf("Enter coefficients a b c: ");
    if (scanf("%lf %lf %lf", &a, &b, &c) != 3) return 0;
    double d = b * b - 4 * a * c;
    if (a == 0) {
        if (b == 0) printf("Not an equation\n");
        else printf("Linear root: %g\n", -c / b);
    } else if (d > 0) {
        printf("Two real roots: %g and %g\n", (-b + sqrt(d)) / (2 * a), (-b - sqrt(d)) / (2 * a));
    } else if (d == 0) {
        printf("One real root: %g\n", -b / (2 * a));
    } else {
        printf("Complex roots: %g+%gi and %g-%gi\n", -b / (2 * a), sqrt(-d) / (2 * a), -b / (2 * a), sqrt(-d) / (2 * a));
    }
    return 0;
 }
