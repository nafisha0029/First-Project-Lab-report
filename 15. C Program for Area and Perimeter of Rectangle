#include <stdio.h>

int main() {
    double length, width;
    printf("Enter length and width of rectangle: ");
    if (scanf("%lf %lf", &length, &width) != 2) return 0;
    double area = length * width;
    double perimeter = 2 * (length + width);
    printf("Area = %.2f\nPerimeter = %.2f\n", area, perimeter);
    return 0;
}
