#include <stdio.h>
#include <string.h>
int main(){
    char *arr[] = {"pear","apple","orange","banana"};
    int n = sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<n-1;i++) for(int j=i+1;j<n;j++) if(strcmp(arr[i],arr[j])>0){
        char *t=arr[i]; arr[i]=arr[j]; arr[j]=t;
    }
    for(int i=0;i<n;i++) printf("%s\n",arr[i]);
    return 0;
}
