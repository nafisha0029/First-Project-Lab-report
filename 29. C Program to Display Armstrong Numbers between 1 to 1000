#include <stdio.h>
#include <math.h>

int main() {
    for(int n = 1; n <= 1000; n++) {
        int temp = n, digits = 0, sum = 0;

        while(temp) { digits++; temp /= 10; }
        temp = n;

        while(temp) {
            sum += pow(temp % 10, digits);
            temp /= 10;
        }

        if(sum == n) printf("%d ", n);
    }
    return 0;
}
